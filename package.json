{
  "name": "puppeteer-utilz",
  "version": "2.0.1",
  "description": "Utility functions for puppeteer",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "dev": "babel src --watch -d lib",
    "test": "jest",
    "prepare": "install-peers",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build:docs": "cd docs/ && yarn install && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibis-vzla/puppeteer-utilz.git"
  },
  "keywords": [
    "Puppeteer",
    "Utility",
    "JavaScript"
  ],
  "author": "Ibis Team <ibis.contacto@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibis-vzla/puppeteer-utilz/issues"
  },
  "homepage": "https://github.com/ibis-vzla/puppeteer-utilz#readme",
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@trivago/prettier-plugin-sort-imports": "2.0.2",
    "eslint": "7.32.0",
    "install-peers-cli": "^2.2.0",
    "jest": "27.0.6",
    "jest-puppeteer": "5.0.4",
    "puppeteer": "10.2.0"
  },
  "peerDependencies": {
    "puppeteer": "^2.0.0"
  },
  "optionalDependencies": {
    "puppeteer-extra-plugin-adblocker": "^2.11.1",
    "puppeteer-extra-plugin-anonymize-ua": "^2.2.6",
    "puppeteer-extra-plugin-block-resources": "^2.2.2",
    "puppeteer-extra-plugin-click-and-wait": "^2.2.2",
    "puppeteer-extra-plugin-devtools": "^2.2.6",
    "puppeteer-extra-plugin-flash": "^2.2.2",
    "puppeteer-extra-plugin-font-size": "^2.2.2",
    "puppeteer-extra-plugin-recaptcha": "^3.1.8",
    "puppeteer-extra-plugin-repl": "^2.2.2",
    "puppeteer-extra-plugin-stealth": "^2.4.5",
    "puppeteer-extra-plugin-user-data-dir": "^2.2.2",
    "puppeteer-extra-plugin-user-preferences": "^2.2.2"
  },
  "dependencies": {
    "async-retry": "^1.3.1",
    "chalk": "^3.0.0",
    "ms": "^2.1.2",
    "no-try": "^1.1.3",
    "puppeteer-extra": "^3.1.9",
    "signale": "^1.4.0"
  }
}
